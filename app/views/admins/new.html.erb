<div class = "admin_container">
	<%= form_for @project do |f| %>
		<%= f.label :name %>
	 	<%= f.text_field :name %><br>	
	 	<%= f.label :description %>
	 	<%= f.text_area :description, size: "30x10" %><br>
	 	<%= f.label :url %>
	 	<%= f.text_field :url%><br>
	 	<%= f.submit "submit", {method: :post}	 %>
	<% end %>
</div>
