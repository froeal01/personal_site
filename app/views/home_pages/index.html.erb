<div class = 'container'>
	<!-- Navbar starts -->
	<div class = "nav_bar">
		<ul>
			<strong><li> Alex Froelich</li></strong>
			<li>    |    </li>
			<li> Full Stack Developer</li> 
		</ul>
	</div>
	<!-- Navbar Ends -->
	<!-- Tile Div Begins -->
	<div class = "link_tiles">
		<!-- Top Row of Tiles Begin -->
		<section class = "link_tiles_top">
			<!-- Resume Nested Divs Begin -->
			<div class = "parent resume_red">
				<p> Projects </p>
				<div class = "child resume_white">
					<a href = "#current_projects_header"><span></span></a>
				</div>
			</div>
			<!-- Resume Nested Divs End -->
			<!-- Unknown Nested Divs Begin -->
			<div class = "parent unknown_red">
				<p> Resume </p>
				<div class = "child unknown_white">
					<a href = "#resume_header"><span></span></a>
				</div>
			</div>
			<!-- Unknown Nested Divs End -->
		</section>
		<!-- Top Row of Tiles End-->
		<!-- Bottom Row of Tiles Begin -->
		<section class = "link_tiles_bottom">
			<div class = "parent git_red">
				<p> Contact </p>
				<div class = "child git_white">
					<a href = "#contact_header"><span></span></a>
				</div>
			</div>
			<!-- <div class = "git_white"></div> -->
			<div class = "parent about_me_red">
				<p> About Me </p>
				<div class = "child about_me_white">
					<a href = "#about_me_header"><span></span></a>
				</div>
			</div>
		</section>
		<!-- Bottom Row of Tiles End -->
	</div>
	<!-- Tile Div Ends -->
	<!-- About_Me_Header Begins -->
	<div id = "about_me_header">
		<p> About Me </p>
	</div>
	<!-- About_Me_Header Ends -->
	<!-- About_Me_Div Begins -->
	<div class = "about_me_div">
		<div class = "me_img_wrapper">
			 <%= image_tag("website_me.jpg", class: "about_me_img") %>
		</div>

		<div class = "about_me_text" >
			 	<p> Energetic and dedicated Ruby on Rails developer</p><br>
			 	<p> Easy going with a great sense of humor</p></br>
			 	<p> Strategy game enthusiast</p></br>
			 	<p> NY Giants and fantasy football fan</p>

		</div>

	</div>
	<!-- About_Me_Div Ends -->
	<!-- Current_Projects Header Starts -->
	<div id = "current_projects_header">
		<p> Current and Past Projects </p>
	</div>
	<!-- Current_Projects Header Ends -->
	<!-- Current_Projects Div Starts -->
	<div class = "current_projects_div">
		<div class = "clear_button"></div>
		<!-- Row Keeper Div Starts -->
			<div class = "project_row_keeper">
				<div class = "ruby_div" >
					<p><%=link_to "Ruby", ruby_projects_path, class: "projects_link", value: "#ruby_projects" %></p>
				</div>
				
				<div class = "ruby_on_rails_div">
					<p><%=link_to "Ruby on Rails", ruby_on_rails_projects_path, class: "projects_link", value: "#ruby_on_rails_projects" %></p>
				</div>

				
				<div class = "javaScript_div">
					<p><%=link_to "javaScript", javascript_projects_path, class: "projects_link", value: "#javaScript_projects" %></p>
				</div>
			</div>
			<!-- Row Keeper Div Ends -->
		<div class = "projects_list"></div>
		<div class = "project_description"></div>

	</div>
	<!-- Current_Projects Div Ends-->
	<!-- Resume Header Starts -->
	<div id = "resume_header">
		<p> Résumé & Social Media </p>
	</div>
	<!-- Resume Header Ends -->
	<!-- Resume Div Starts -->
	<div class = "resume_div">
		<div class	= "resume_row_keeper">
			<div class = "linkedin_div">
				<a href="http://linkedin.com/pub/alex-froelich/78/b44/766/"><%= image_tag('linkedin-logo.jpg', size: "100x100")%></a>
				<p> linkedin </p>
			</div>
			<div class = "resume_pdf">
			<a href = '/resume' target="_blank" ><%= image_tag('Resume_web2.png',size: "216x216") %></a>
			<p> Résumé </p>
			</div>
		</div>
	</div>
	<!-- Resume Div Ends-->
	<!-- Contact Header Starts -->
	<div id = "contact_header">
		<p> Contact Me </p>
	</div>
	<!-- Contact Header Ends -->
	<!-- Contact Div Starts -->
	<div class = "contact_div">
		<% if flash[:notice] %>
		<div class = "flash"><%= flash[:notice] %></div>
	<% end %>
		<%= render "contact/form" %>

	</div>
	<!-- Contact Div Ends-->
</div>
<!-- Container Closes -->